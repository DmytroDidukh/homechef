{
  "name": "homechef",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "lint:css": "stylelint src/**/*.scss",
    "lint:css:staged": "stylelint --fix",
    "lint:fix": "npm run lint:js && npm run lint:tsx && npm run lint:css -- --fix",
    "lint:js": "eslint --fix --ext .js ./src",
    "lint:ts": "eslint --fix --ext .ts ./src",
    "lint:tsx": "eslint --fix --ext .tsx ./src",
    "prepare": "husky install",
    "lint-staged": "lint-staged",
    "storybook": "start-storybook -p 6006 -s public",
    "build-storybook": "build-storybook -s public"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.js": [
      "prettier --write",
      "npm run lint:js"
    ],
    "*.ts": [
      "prettier --write",
      "npm run lint:ts"
    ],
    "*.tsx": [
      "prettier --write",
      "npm run lint:tsx"
    ]
  },
  "stylelint": {
    "extends": "./linters/stylelintrc.json"
  },
  "dependencies": {
    "@reduxjs/toolkit": "~1.8.2",
    "@types/node": "~16.11.41",
    "classnames": "~2.3.1",
    "connected-react-router": "~6.9.2",
    "firebase": "~9.8.4",
    "react": "~18.2.0",
    "react-dom": "~18.2.0",
    "react-redux": "~8.0.2",
    "react-router": "~6.3.0",
    "react-router-dom": "~6.3.0",
    "react-scripts": "5.0.1",
    "react-select": "~5.4.0",
    "redux-thunk": "~2.4.1",
    "typescript": "~4.7.4"
  },
  "devDependencies": {
    "@babel/eslint-parser": "~7.18.2",
    "@babel/preset-react": "~7.18.6",
    "@babel/preset-typescript": "~7.18.6",
    "@storybook/addon-actions": "^6.5.9",
    "@storybook/addon-essentials": "^6.5.9",
    "@storybook/addon-interactions": "^6.5.9",
    "@storybook/addon-links": "^6.5.9",
    "@storybook/builder-webpack5": "^6.5.9",
    "@storybook/manager-webpack5": "^6.5.9",
    "@storybook/node-logger": "^6.5.9",
    "@storybook/preset-create-react-app": "~4.1.2",
    "@storybook/preset-scss": "~1.0.3",
    "@storybook/react": "^6.5.9",
    "@storybook/testing-library": "0.0.13",
    "@types/react": "~18.0.14",
    "@types/react-dom": "~18.0.5",
    "@types/react-router": "~5.1.18",
    "@types/react-router-dom": "~5.3.3",
    "@typescript-eslint/eslint-plugin": "~5.30.0",
    "@typescript-eslint/parser": "~4.33.0",
    "babel-plugin-named-exports-order": "0.0.2",
    "eslint": "~8.19.0",
    "eslint-config-prettier": "~8.5.0",
    "eslint-config-standard": "~16.0.3",
    "eslint-plugin-babel": "~5.3.1",
    "eslint-plugin-import": "~2.26.0",
    "eslint-plugin-jsx": "~0.1.0",
    "eslint-plugin-jsx-a11y": "~6.6.0",
    "eslint-plugin-node": "~11.1.0",
    "eslint-plugin-prettier": "~4.1.0",
    "eslint-plugin-promise": "~5.2.0",
    "eslint-plugin-react": "~7.30.1",
    "eslint-plugin-react-hooks": "~4.6.0",
    "eslint-plugin-storybook": "~0.6.1",
    "husky": "~8.0.1",
    "lint-staged": "~13.0.3",
    "prettier": "~2.7.1",
    "prettier-eslint": "~15.0.1",
    "prop-types": "^15.8.1",
    "sass": "~1.53.0",
    "sass-loader": "~7.3.0",
    "storybook-css-modules": "~1.0.8",
    "style-loader": "~0.23.1",
    "ts-loader": "~5.4.5",
    "tsconfig-paths-webpack-plugin": "~4.0.0",
    "tslint-plugin-prettier": "~2.3.0",
    "typescript-plugin-css-modules": "~3.4.0",
    "webpack": "^5.74.0"
  },
  "eslintConfig": {
    "overrides": [
      {
        "files": [
          "**/*.stories.*"
        ],
        "rules": {
          "import/no-anonymous-default-export": "off"
        }
      }
    ]
  }
}
